<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <template id="bs3_demo_assets_editor" inherit_id="website.assets_editor" name="Website Editor">
      <xpath expr="." position="inside">
        <script type="text/javascript" src="/bs_demo/static/src/js/bs3_collapse_demo.js"></script>
      </xpath>
    </template>
    <template id="bs3_demo_assets_frontend" inherit_id="website.assets_frontend" name="Website assets_frontend">
      <xpath expr="//script[last()]" position="after">
        <script type="text/javascript" src="/bs_demo/static/src/js/website_bs_signature.js"></script>
      </xpath>
    </template>
  </data>
</odoo>
